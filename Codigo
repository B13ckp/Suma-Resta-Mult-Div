#include <stdio.h>

int main() { 

	int num1 = 5;
	int num2 = 10;
	
	float resta,suma, mult, division;
	
	resta = num1 - num2;
	printf("\nEl resultado de la resta es: %.2f", resta);
	
	suma = num1 + num2;
	printf("\nEl resultado de la suma es: %.2f",suma);
	
	mult = num1 * num2;
	printf("\nEl resultado de la multiplicacion es: %.2f",mult);
	
	division = num2 / num1;
	printf("\nEl resultado de la division  es: %.2f",division);
	
	return 0;
	
}
